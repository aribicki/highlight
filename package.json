{ "name"    : "SPA",          // name of project           
  "version" : "0.0.3",          // used by npm to make sure right package installed
  "private" : true,     // npm will refuse to publish it when set to true
  "dependencies" : {  // maps a package name to a version range
    "express"   : "4.10.x", //require express module as dependency
    "body-parser": "1.5.x", // returns middleware that only parses json
